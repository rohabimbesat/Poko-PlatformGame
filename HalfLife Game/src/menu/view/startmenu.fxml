            <Image url="@title/poko.png" />
         </image></ImageView>
         <Label /> <Label /> <Label />
		<Button onAction="#goStart" text="START" />
		<Label />
		<Button onAction="#goExit" text="EXIT" />
		<Label />
   </children>
</VBox>
